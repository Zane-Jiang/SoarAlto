{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/benchmark/gapbs/bc",
             "args": ["-f","/home/jz/public_bench/gapbs/GAP-urand.sg","-i4","-n1"],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [{"name": "LD_PRELOAD","value": "/home/jz/PaperLab/SoarAlto/src/soar/interc/ldlib.so"}],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "cwd": "${workspaceFolder}/src/soar/run/rst",
            "python.pythonPath": "{workspaceFolder}/venv/bin/python",
            "program": "${file}",
            "args": ["/home/jz/profdata/"],
            "console": "integratedTerminal"

        }
        ,
    ]

}